{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            p.id,\n            p.author_name,\n            p.author_hash,\n            p.title,\n            p.created_at,\n            (SELECT COUNT(*) FROM comments c WHERE c.post_id = p.id AND c.deleted_at IS NULL) AS \"comment_count!\"\n        FROM posts p\n        WHERE category_id = $1 AND deleted_at IS NULL\n        ORDER BY p.created_at DESC\n        LIMIT $2 OFFSET $3\n        ",
