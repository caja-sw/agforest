{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            p.id,\n            p.author_name,\n            p.author_hash,\n            p.category_id,\n            c.name AS category_name,\n            c.readonly AS category_readonly,\n            p.title,\n            p.content,\n            p.created_at\n        FROM posts p\n        INNER JOIN categories c ON p.category_id = c.id\n        WHERE p.id = $1 AND p.deleted_at IS NULL\n        ",
